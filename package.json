    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.0",
    "sharp": "^0.32.0",
    "swagger-ui-express": "^4.6.2",
    "util": "^0.12.5",
    "yup": "^1.1.0",
